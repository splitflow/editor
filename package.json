{
  "name": "@splitflow/editor",
  "version": "1.0.0",
  "description": "Splitflow editor",
  "author": "team@splitflow.io",
  "homepage": "https://github.com/splitflow/editor#readme",
  "bugs": {
    "url": "https://github.com/splitflow/editor/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/splitflow/editor.git"
  },
  "type": "module",
  "files": [
    "dist/**/*.js",
    "dist/**/*.cjs",
    "dist/**/*.d.ts",
    "src/svelte/**/*.js",
    "src/svelte/**/*.svelte"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.umd.cjs",
      "types": "./dist/lib/index.d.ts"
    },
    "./svelte": {
      "svelte": "./src/svelte/index.js"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/lib/index.d.ts"
      ]
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "test": "vitest"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^2.0.3",
    "@tsconfig/svelte": "^4.0.1",
    "prettier": "^3.0.1",
    "prettier-plugin-svelte": "^3.0.3",
    "svelte": "^3.57.0",
    "svelte-check": "^2.10.3",
    "tslib": "^2.5.0",
    "typescript": "^5.0.2",
    "vite": "^4.3.2",
    "vite-plugin-css-injected-by-js": "^3.3.0",
    "vite-plugin-dts": "^3.2.0",
    "vitest": "^0.31.4"
  },
  "dependencies": {
    "@splitflow/app": "^1.1.0",
    "@splitflow/core": "file:../core",
    "@splitflow/css-reset": "^1.1.0",
    "@splitflow/designer": "^2.1.0",
    "@splitflow/lib": "^1.1.0"
  }
}
